{
    "name": "react-dynamic-forms",
    "version": "0.16.1",
    "description": "Dynamic forms library for React",
    "license": "BSD-3-Clause-LBNL",
    "main": "lib/index.js",
    "author": "ESnet Tools Team <tools@es.net>",
    "repository": {
        "type": "git",
        "url": "git://github.com:esnet/esnet-react-forms.git"
    },
    "bugs": {
        "url": "https://github.com/esnet/esnet-react-forms/issues"
    },
    "scripts": {
        "lint": "eslint src/components/*.js",
        "test": "react-scripts test --env=jsdom",
        "build": "echo \"*** Building lib\n\" && rm -rf lib && mkdir lib && mkdir lib/components && babel src/forms --optional runtime --stage 0 --ignore __tests__/* --out-dir lib/ && mkdir lib/components/css && cp ./src/forms/components/css/*.css ./lib/components/css/ && babel src/forms/index.js --optional runtime --stage 0 --out-file lib/index.js",
        "start-website": "react-scripts start",
        "build-website": "echo \"*** Building website\n\" && rm -rf docs && react-scripts build && mv build docs",
        "precommit": "lint-staged",
        "prettier": "prettier --print-width 100 --tab-width 4 --write \"src/**/*.js\""
    },
    "lint-staged": {
        "src/**/*.js": ["prettier --print-width 100 --tab-width 4 --write", "git add"]
    },
    "dependencies": {
        "classnames": "^2.1.3",
        "deepcopy": "^0.5.0",
        "flexbox-react": "^4.2.1",
        "immutable": "^3.8.1",
        "invariant": "^2.1.1",
        "jquery": "^2.1.1",
        "keymirror": "^0.1.1",
        "moment": "^2.10.6",
        "react-addons-css-transition-group": "15.3.2",
        "react-addons-shallow-compare": "15.3.2",
        "react-bootstrap": "^0.30.7",
        "react-datepicker": "^0.39.0",
        "react-immutable-proptypes": "^2.1.0",
        "react-virtualized-select": "^2.2.0",
        "revalidator": "^0.3.1",
        "string-hash": "^1.1.0",
        "underscore": "^1.8.3"
    },
    "peerDependencies": {
        "react": "^15.3.1"
    },
    "devDependencies": {
        "babel-cli": "^6.5.1",
        "babel-core": "^6.5.2",
        "babel-loader": "^6.2.3",
        "babel-plugin-transform-class-properties": "^6.22.0",
        "babel-preset-es2015": "^6.5.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-0": "^6.5.0",
        "chance": "^1.0.6",
        "enzyme": "^2.7.0",
        "enzyme-to-json": "^1.4.5",
        "eslint-config-prettier": "^2.3.0",
        "eslint-plugin-prettier": "^2.1.2",
        "eslint-plugin-react": "^7.1.0",
        "husky": "^0.14.3",
        "lint-staged": "^4.0.2",
        "merge": "^1.2.0",
        "prettier": "^1.5.3",
        "react": "^15.3.1",
        "react-addons-test-utils": "^15.4.2",
        "react-docgen": "^2.9.1",
        "react-dom": "^15.3.1",
        "react-markdown": "^1.0.5",
        "react-router": "^2.6.1",
        "react-scripts": "^0.8.3",
        "react-test-renderer": "^15.4.1",
        "underscore": "^1.8.3",
        "webpack": "^1.14.0"
    },
    "keywords": ["forms", "dynamic", "react"],
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "testPathIgnorePatterns": ["<rootdir>/src/__tests__/src"]
    }
}
